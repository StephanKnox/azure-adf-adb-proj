{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "taxinycdf"
		},
		"ADLSGen2_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2_ls'"
		},
		"ADLSGen2_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://terraformazurezurich1.dfs.core.windows.net/"
		},
		"tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filepath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"tr_on_file_upload_to_landing_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/914e6dc1-4708-45e0-8c9e-b29c1b4faec8/resourceGroups/rg-terraform/providers/Microsoft.Storage/storageAccounts/terraformazurezurich1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "taxinycdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_ls_accountKey": {
						"value": "[parameters('ADLSGen2_ls_accountKey')]"
					},
					"ADLSGen2_ls_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_ls_properties_typeProperties_url')]"
					},
					"tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filepath": {
						"value": "[parameters('tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filepath')]"
					},
					"tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filename": {
						"value": "[parameters('tr_on_file_upload_to_landing_properties_pl_csv_to_parquet_ingestion_template_parameters_filename')]"
					},
					"tr_on_file_upload_to_landing_properties_typeProperties_scope": {
						"value": "[parameters('tr_on_file_upload_to_landing_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}